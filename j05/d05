insert into ft_table (login,groupe,date_de_creation) select nom, 'other',date_naissance from fiche_personne WHEre nom like '%a%' && lenght(nom) < 9 order by nom limit 10;

ex4
update ft_table set date_de_creation =DATE_ADD(date_de_creation, interval 20 year) where id > 5;

ex5
DELETE FROM ft_table
WHERE id < 6

ex06
SELECT titre, resum FROM film
WHERE film.resum LIKE '%vincent%'
ORDER BY film.id_film ASC

ex07
SELECT titre, resum FROM film
WHERE (titre LIKE '%42%' || resum LIKE '%42%')
ORDER BY duree_min ASC

ex08
SELECT nom, prenom, DATE( date_naissance ) AS 'date de naissance' FROM fiche_personne
WHERE YEAR( date_naissance )=1989
ORDER BY nom ASC

ex09
SELECT COUNT(*) AS 'nb_court-metrage' FROM film
WHERE duree_min <= 42
