CREATE TABLE  ft_table ( id INT PRIMARY KEY NOT NULL AUTO_INCREMENT, login VARCHAR(8) NOT NULL DEFAULT 'toto', groupe ENUM ('staff', 'student', 'other') NOT NULL, date_de_creation DATE NOT NULL );


INSERT INTO ft_table (login, date_de_creation) (SELECT nom, date_naissance FROM fiche_personne WHERE CHAR_LENGTH(nom) < 9);

insert into ft_table (id-genre, login) values("loki")

’loki’ est un ’staff’, crée le ’2013-05-01’


